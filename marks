package com.mycompany.studentmarks;
import java.util.Scanner;

public class StudentMarks{
    
  public static void main(String[] args) {
    Scanner scanner = new Scanner(System.in);

    System.out.println("input student marks: ");
    int marks = scanner.nextInt();

    if (marks >= 80) {
      System.out.println("level 7-Outstanding achievement");
    } else if (marks >= 70) {
      System.out.println("level 6-Meritorious achievement");
    } else if (marks >= 60) {
      System.out.println("level 5-Substantical achievement");
    } else if (marks >=50){
      System.out.println("level 4-Moderate achievement");
    } else if (marks >=40){
     System.out.println("level 3-Adequate achievement");
    } else if(marks >=30){
      System.out.println("level 2-Elementary achievement");
        
    } else {
      System.out.println("level 1-Not achieved");
              
    }
  }}


